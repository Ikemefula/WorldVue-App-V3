{
   "presets": [
      ["@babel/preset-env", {
         "modules": false,
         "targets": {
            "browsers": [
               "> 0.25%, not dead"
            ]
         }
      }],
      [
         "@babel/preset-react",
         {
            "runtime": "automatic",
            "importSource": "@emotion/react"
         }
      ]
   ],
   "plugins": [
      "@emotion",
      ["@babel/plugin-transform-runtime", {
         "regenerator": true
      }],
      "@babel/plugin-proposal-class-properties"]
}